package Datos;

import java.io.Serializable;
public class Persona implements Serializable{
   private String nif;
   private int ciclo;
   private int curso;
   private String nombre;
   public Persona (String nif, int ciclo, int curso, String nombre) { 
	   this.nombre = nombre;     
	   this.nif = nif;
	   this.ciclo = ciclo;
	   this.curso = curso;
	   }
   public Persona(){ 
	   this.nombre = null;    
	   }
   public void setNombre (String nombre) { 
	   this.nombre = nombre;
	   }
   public void setEdad(int edad) {
	   this.edad = edad;
	   }
   public String getNombre() {
	   return this.nombre;}
   
   public int getEdad() {return this.edad;}
}
