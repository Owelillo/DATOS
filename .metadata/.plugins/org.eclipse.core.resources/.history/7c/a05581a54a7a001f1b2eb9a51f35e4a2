package VocalNumeroUsuario;

public class Terminal {
	private static Terminal instancia;
	private Numero numero;
	private Vocal vocal;
	private Usuario usuario;

		public Terminal() {
			System.out.println("Terminal creada");	
			}
		public static Terminal getInstancia() {
			
			if(instancia == null) {
				instancia = new Terminal();
			}
				return instancia;
		}
}
