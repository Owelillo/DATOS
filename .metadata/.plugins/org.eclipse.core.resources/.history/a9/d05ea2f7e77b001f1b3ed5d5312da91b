package Datos;

import java.io.Serializable;
public class Persona implements Serializable{
   private String nif;
   private int ciclo;
   private int curso;
   private String nombre;
   public Persona (String nif, int ciclo, int curso, String nombre) { 
	   this.nombre = nombre;     
	   this.nif = nif;
	   this.ciclo = ciclo;
	   this.curso = curso;
	   }
   public Persona(){ 
	   this.nombre = null;    
	   }
   public String getNif() {
	return nif;
}
public void setNif(String nif) {
	this.nif = nif;
}
public int getCurso() {
	return curso;
}
public void setCurso(int curso) {
	this.curso = curso;
}
public int getCiclo() {
	return ciclo;
}

}
